{
  "expo": {
    "name": "frontend",
    "slug": "frontend",
    "version": "1.0.0",
    "orientation": "portrait",

    "icon": "./assets/images/fixo-logo.png",

    "userInterfaceStyle": "light",
    "newArchEnabled": false,

    "splash": {
      "image": "./assets/images/fixo-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },

    "ios": {
      "supportsTablet": true
    },

    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/fixo-logo.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },

    "web": {
      "favicon": "./assets/images/fixo-logo.png"
    },

    "assetBundlePatterns": ["**/*"]
  }
}
